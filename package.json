  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "asset-revision-webpack-plugin": "^1.0.0",
    "axios": "^1.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "framer-motion": "^12.23.26",
    "html-webpack-plugin": "^5.6.3",
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.10.0",
    "react-script": "^2.0.5",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.9",
    "web-vitals": "^3.3.0",
    "webpack": "^5.99.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rm -rf ./build",
    "build": "webpack --config webpack.config.js && react-scripts build --source-map",
    "postbuild": "chmod -R 755 ./build && php build/generate-htaccess.php && rm -f build/generate-htaccess.php && echo '\n\u001b[33m⚠️  WARNING: When archiving the build folder, make sure to enable \"Show hidden files\"\u001b[0m' && echo '\u001b[33m   otherwise the .htaccess file will not be included in the archive!\u001b[0m\n'",
    "deploy": "dotenv -e .env -- yarn run deploy:raw",
    "deploy:raw": "yarn build && scp -P $DEPLOY_PORT -r ./build/. $DEPLOY_USER@$DEPLOY_HOST:$DEPLOY_PATH && echo 'Deploy successful' || echo 'Deploy failed'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "dotenv-cli": "^8.0.0",
    "file-loader": "^6.2.0",
    "raw-loader": "^4.0.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
