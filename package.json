{
  "name": "lors",
  "version": "0.1.0",
  "private": true,
  "homepage": "/html_lors_old",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "asset-revision-webpack-plugin": "^1.0.0",
    "axios": "^1.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "mini-css-extract-plugin": "^2.7.5",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-cookie-consent": "^8.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-router-dom": "^6.10.0",
    "react-script": "^2.0.5",
    "react-scripts": "^5.0.1",
    "styled-components": "^5.3.9",
    "web-vitals": "^3.3.0",
    "webpack": "^5.77.0"
  },
  "scripts": {
    "clean": "rm -rf ./build",
    "start": "react-scripts start",
    "build": "webpack --config webpack.config.js && react-scripts build --source-map",
    "deploy": "dotenv -e .env -- yarn run deploy:raw",
    "deploy:raw": "yarn build && scp -P $DEPLOY_PORT -r ./build/* $DEPLOY_USER@$DEPLOY_HOST:$DEPLOY_PATH && echo 'Deploy successful' || echo 'Deploy failed'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postbuild": "chmod -R 755 ./build && php build/generate-htaccess.php && rm -f build/generate-htaccess.php"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "dotenv-cli": "^8.0.0",
    "webpack-cli": "^5.0.1"
  }
}
